zuul:
  routes:
    # Route for your backend service
    my-service:
      path: /student/**
      url: http://localhost:8090 # Your downstream service

# Google Cloud OAuth2 Configuration (used by the custom filter)
oauth2:
  google:
    token-uri: https://oauth2.googleapis.com/token
    scope: 'https://www.googleapis.com/auth/userinfo.email'
    
# Key for storing the original URI
custom:
  original-uri-key: "originalRequestUri"
server:
  port=8080